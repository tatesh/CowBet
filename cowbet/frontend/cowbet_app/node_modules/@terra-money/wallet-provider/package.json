  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "typings": "index.d.ts",
  "name": "@terra-money/wallet-provider",
  "version": "3.8.0",
  "dependencies": {
    "@terra-money/web-extension-interface": "^3.8.0",
    "@terra-money/use-wallet": "^3.8.0",
