      "node": "./_commonjs/StaticWalletProvider.js",
      "default": "./StaticWalletProvider.js"
    },
    "./StaticWalletProvider.js": {
      "import": "./StaticWalletProvider.js",
      "require": "./_commonjs/StaticWalletProvider.js",
      "node": "./_commonjs/StaticWalletProvider.js",
