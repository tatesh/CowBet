  },
  "main": "./_commonjs/index.js",
  "module": "./index.js",
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./_commonjs/index.js",
      "node": "./_commonjs/index.js",
      "default": "./index.js"
    },
    "./public/*": "./public/*",
    "./README.md": "./README.md",
    "./StaticWalletProvider": {
      "import": "./StaticWalletProvider.js",
